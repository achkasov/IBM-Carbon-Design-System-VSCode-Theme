    "tokenColors": [
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown",
                "variable.legacy.builtin.python"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#6929c4"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "variable.other.enummember",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#161616"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#0043ce"
            }
        },
        {
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.less"
            ],
            "settings": {
                "foreground": "#0043ce"
            }
        },
        {
            "scope": "entity.other.attribute-name",
